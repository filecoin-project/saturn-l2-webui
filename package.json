{
  "name": "saturn-webui",
  "private": true,
  "version": "0.0.24",
  "description": "A frontend UI for the Saturn L2 node.",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint src/ test/",
    "test": "npm run build && vitest",
    "test:unit": "npm run build && vitest test/unit",
    "test:e2e": "npm run build && vitest test/e2e",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@glif/filecoin-address": "^2.0.0-beta.3",
    "bytes": "^3.1.2",
    "chart.js": "^3.8.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.14",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "puppeteer": "^14.1.2",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.13",
    "vitest": "^0.13.0"
  },
  "author": "Protocol Labs",
  "license": "(Apache-2.0 AND MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filecoin-project/saturn-webui.git"
  },
  "bugs": {
    "url": "https://github.com/filecoin-project/saturn-webui/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{html,json,css,md}": "prettier --write"
  }
}
